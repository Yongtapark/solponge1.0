<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="collection_of_sql_job_scrap">
    <select id="selected_scrap" resultType="com.solponge.domain.JobScrap.JobScrapVO">
        SELECT * FROM JOBSCRAP WHERE MEMBER_NO=#{MEMBER_NO}
        </select>
    <insert id="insert_scrap_company">
        INSERT INTO JOBSCRAP (MEMBER_NO, companyName, scrap_time)
        VALUES (#{MEMBER_NO},#{companyName},SYSDATE)
    </insert>
</mapper>